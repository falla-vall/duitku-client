{
  "name": "duitku-client",
  "version": "0.0.0",
  "description": "My awesome typescript library",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build-fast": "tsup src/index.ts --format cjs,esm",
    "build": "pnpm run build-fast -- --dts",
    "test": "vitest run",
    "prepublishOnly": "pnpm run build",
    "prepare": "husky install"
  },
  "license": "MIT",
  "prettier": "@flla/prettier-config",
  "lint-staged": {
    "**/*.{js,ts,json}": [
      "pnpm prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@flla/prettier-config": "1.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/luxon": "^3.3.0",
    "dotenv": "^16.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "2.8.4",
    "tsup": "6.6.3",
    "typescript": "4.9.5",
    "vitest": "0.28.5"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "crypto-js": "^4.1.1",
    "luxon": "^3.3.0"
  }
}
